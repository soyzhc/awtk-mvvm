{
  "name":"shape",
  "desc":"shape info",
  "props": [
    {
      "name":"type",
      "init_value":1,
      "type":"int32_t"
    },
    {
      "name":"x",
      "init_value":10,
      "type":"int32_t"
    },
    {
      "name":"y",
      "init_value":20,
      "type":"int32_t"
    },
    {
      "name":"w",
      "init_value":30,
      "type":"int32_t"
    },
    {
      "name":"h",
      "init_value":40,
      "type":"int32_t"
    },
    {
      "name":"opacity",
      "init_value":50,
      "type":"int32_t"
    },
    {
      "name":"text_align",
      "init_value":1,
      "type":"int32_t",
      "setter":true,
      "getter":true
    },
    {
      "name":"name",
      "type":"char*",
      "setter":true,
      "getter":true
    },
    {
      "name":"overview",
      "type":"char*",
      "getter":true
    }
  ],
  "cmds": [
    {
      "name":"change_type",
      "canExec": true,
      "desc":"change shape type"
    },
    {
      "name":"save",
      "canExec": "return shape->name.size > 0;",
      "desc":"save shape info"
    }
  ]
}
